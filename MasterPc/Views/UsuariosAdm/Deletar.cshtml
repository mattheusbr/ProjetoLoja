@model MasterPc.Models.Usuario

<title>MasterPcAdmin - Deletar Produto</title>

@using (Html.BeginForm(("Deletar", "UsuariosAdm", FormMethod.Post)))
{
    @Html.AntiForgeryToken()
    <div class="card mb-3">
        <div class="card-header">
            <h4>Você deseja deletar esse produto ?</h4>
        </div>
        <div class="card-body">
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.Nome)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Nome)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.CPF)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.CPF)
                </dd>

                <dt>
            </dl>
            <p>
                <div class="form-actions no-color">
                    <input type="submit" value="Sim" class="btn btn-danger" />
                    <a align="bottom" href="~/Admin/Usuario/Listar" class="btn btn-primary">Não</a>
                </div>
            </p>
        </div>
        <div class="card-footer small text-muted">Horario de Alteração @DateTime.Now </div>
    </div>
}

